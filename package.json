{
  "name": "react-native-mostly-good-metrics",
  "version": "0.1.0",
  "description": "React Native SDK for MostlyGoodMetrics analytics",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "react-native-mostly-good-metrics.podspec",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!ios/build",
    "!android/build"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "clean": "del-cli lib"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "analytics",
    "metrics",
    "mostlygoodmetrics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mostly-Good-Metrics/mostly-good-metrics-react-native.git"
  },
  "author": "Josh Holtz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mostly-Good-Metrics/mostly-good-metrics-react-native/issues"
  },
  "homepage": "https://github.com/Mostly-Good-Metrics/mostly-good-metrics-react-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "del-cli": "^5.0.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-builder-bob": "^0.23.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
